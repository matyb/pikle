<?xml version="1.0"?>
<config>
	<datasources>
		<datasource>
			<name>the test db</name>
			<driver>com.mysql.jdbc.Driver</driver>
			<url>jdbc:mysql://localhost:3306/test</url>
			<databaseDialectName>com.pk.MySQLDialect</databaseDialectName>
		</datasource>
	</datasources>
	<export>
		<delimiterChar>,</delimiterChar>
		<encloseByChar>"</encloseByChar>
		<useCdata>true</useCdata>
		<headerColor>#ffff80</headerColor>
		<borderColor>green</borderColor>
		<altRowColor>#b9ffb9</altRowColor>
	</export>
	<general>
		<lafs>
			<laf>
				<class>javax.swing.plaf.metal.MetalLookAndFeel</class>
				<selected>true</selected>
			</laf>
			<laf>
				<class>com.sun.java.swing.plaf.windows.WindowsLookAndFeel</class>
				<selected>false</selected>
			</laf>
			<laf>
				<class>com.sun.java.swing.plaf.motif.MotifLookAndFeel</class>
				<selected>false</selected>
			</laf>
		</lafs>
			<history>
				<mode>2</mode>
			</history>
</general>
</config>
